<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
 auto-import="true" assembly="QSBanks" namespace="QSBanks">
	<class name="Bank" lazy="false" table="banks">
		<id name="Id" column="id">
			<generator class="native"></generator>
	 	</id>
	 	<property name="Name" column="name" />
	 	<property name="Bik" column="bik" />
	 	<property name="CorAccount" column="cor_account" />
	 	<many-to-one name="Region" class="BankRegion" column="region_id" />
	 	<property name="City" column="city" />
	 	<property name="Deleted" column="deleted" />
 	</class>
 	<class name="BankRegion" lazy="false" table="bank_regions">
		<id name="Id" column="id">
			<generator class="native"></generator>
	 	</id>
	 	<property name="Region" column="region" />
	 	<property name="City" column="city" />
	 	<property name="RegionNum" column="region_num" />
 	</class>
</hibernate-mapping>