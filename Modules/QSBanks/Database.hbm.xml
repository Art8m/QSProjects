<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
 auto-import="true" assembly="QSBanks" namespace="QSBanks">
	<class name="Bank" lazy="false" table="banks">
		<id name="Id" column="id">
			<generator class="native"></generator>
	 	</id>
	 	<property name="Name" column="name" />
	 	<property name="Bik" column="bik" />
	 	<property name="CorAccount" column="cor_account" />
	 	<property name="Region" column="region" />
	 	<property name="City" column="city" />
	 	<property name="Deleted" column="deleted" />
 	</class>
 	<class name="Account" lazy="false" table="accounts">
		<id name="Id" column="id">
			<generator class="native"></generator>
	 	</id>
	 	<property name="Name" column="name" />
	 	<property name="Number" column="number" />
	 	<property name="Inactive" column="inactive" />
	 	<many-to-one name="InBank" class="Bank" column="bank_id" />
 	</class>

</hibernate-mapping>